<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="report_cicon_cheque_receipt_template">
        <t t-call="web.html_container">
                <t t-call="web.internal_layout">
                    <div class="page">
                        <div>
                            <table class="table table-bordered">
                                <caption class="text-center"> Cheque Receipt  </caption>
                                <thead>
                                    <tr>
                                        <th>Customer </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr t-foreach="docs" t-as="o">
                                        <td> <span t-field="o.partner_id.name"/> </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                   </div>
            </t>
        </t>
    </template>


     <template id="report_cicon_bank_deposit_slip_template">
        <t t-call="web.html_container">
                <t t-call="web.internal_layout">
                    <div class="page">
                        <div>
                            <table class="table table-bordered">
                                <caption class="text-center"> Bank Deposit Slip  </caption>
                                <thead>
                                    <tr>
                                        <th>Customer </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr t-foreach="docs" t-as="o">
                                        <td> <span t-field="o.partner_id.name"/> </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                   </div>
            </t>
        </t>
    </template>


</odoo>